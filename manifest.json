{
  "name": "Request Pipeline",
  "version": "1.0.2",
  "manifest_version": 3,
  "description": "This is an extension that forwards web interface request information to a specified server",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
 "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "tabs",
    "debugger"
    ],
    "background": {
      "service_worker": "background.js"
    },
  "action": {
    "default_popup": "popup.html"
  }
}
